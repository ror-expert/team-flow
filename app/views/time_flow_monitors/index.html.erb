<p id="notice"><%= notice %></p>

<h1>Time Flow Monitors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_flow_monitors.each do |time_flow_monitor| %>
      <tr>
        <td><%= time_flow_monitor.name %></td>
        <td><%= time_flow_monitor.description %></td>
        <td><%= time_flow_monitor.project_id %></td>
        <td><%= link_to 'Show', time_flow_monitor %></td>
        <td><%= link_to 'Edit', edit_time_flow_monitor_path(time_flow_monitor) %></td>
        <td><%= link_to 'Destroy', time_flow_monitor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time Flow Monitor', new_time_flow_monitor_path %>
